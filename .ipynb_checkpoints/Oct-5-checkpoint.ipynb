   "source": [
    "def sum_of_numbers(n):\n",
    "    # Base case\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    # Recursive case\n",
    "    return n + sum_of_numbers(n - 1)\n",
    "\n",
    "# Call the function\n",
    "result = sum_of_numbers(5)\n",
    "print(\"Sum of first 5 numbers is:\", result)\n"
   ]
